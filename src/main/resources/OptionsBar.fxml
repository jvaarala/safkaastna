<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>

<ToolBar xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="view.OptionsBarController">
    <items>
        <Button mnemonicParsing="false" onAction="#refresh" text="Connect"/>
        <Button mnemonicParsing="false" onAction="#help" text="Help"/>
        <Button mnemonicParsing="false" onAction="#changeFI" text="FI" />
        <Button mnemonicParsing="false" onAction="#changeSWE" text="SWE" />
        <Button mnemonicParsing="false" onAction="#changeENG" text="ENG" />
    </items>
</ToolBar>
